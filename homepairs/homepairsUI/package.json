{
  "main": "src/App.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "images": "node scripts/images.js",
    "fonts": "node scripts/fonts.js"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/setup/test-shim.js",
      "<rootDir>/tests/setup/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "preset": "ts-jest/presets/js-with-ts",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/setup/fileMock.js",
      "\\.(css|less)$": "<rootDir>/tests/setup/styleMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testMatch": [
      "**/tests/**/*.test.(ts|tsx|js)"
    ],
    "collectCoverage": false
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "axios": "^0.19.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "expo": "^35.0.0",
    "expo-font": "~7.0.0",
    "modal-enhanced-react-native-web": "^0.2.0",
    "react": "16.9.0",
    "react-art": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.1.tar.gz",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-material-menu": "^1.0.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-web": "~0.11.7",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.25",
    "@types/react": "^16.9.17",
    "@types/react-native": "^0.60.27",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^16.9.1",
    "babel-preset-expo": "^8.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "jest": "^24.9.0",
    "jest-expo": "^36.0.0",
    "jest-fetch-mock": "^3.0.1",
    "react-native-testing-library": "^1.12.0",
    "react-test-renderer": "^16.12.0",
    "redux-devtools": "^3.5.0",
    "redux-mock-store": "^1.5.4",
    "redux-tools": "^3.2.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "private": true
}
